{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/register",
      "methods": ["POST"],
      "dest": "index.js"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "index.js"
    }
  ],
  "env": {
    "MONGODB_URI": "@MONGODB_URI"
  }
}